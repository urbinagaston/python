#Ejemplo Hola Mundo.
print("¡Hola, Mundo!")

#Ejemplo Hola Mundo con salto de línea.
print("¡Hola\nMundo!")

#Ejemplo Hola Mundo con barra.
print("¡Hola \\ Mundo!")

#Ejemplo print con varios argumentos.
print("Practicando","con","python")

#Ejemplo print con varios argumentos.
print("Practicando","con","python")

#Ejemplo print con end.
print("Mi nombre es", "Python.", end=" ")
print("Monty Python.")

#Ejemplo sep & end
print("Mi", "nombre", "es", sep="_", end="*")
print("Monty", "Python.", sep="*", end="*\n")
